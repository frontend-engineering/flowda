{
  "name": "nx-plugin-flowda",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nx-plugin-flowda/src",
  "projectType": "library",
  "targets": {
    "dev": {
      "executor": "@flowda/nx-plugin-flowda:dev",
      "options": {
        "yalc": false,
        "bundleDts": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nx-plugin-flowda",
        "main": "libs/nx-plugin-flowda/src/index.ts",
        "tsConfig": "libs/nx-plugin-flowda/tsconfig.lib.json",
        "assets": [
          "libs/nx-plugin-flowda/*.md",
          {
            "input": "./libs/nx-plugin-flowda/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/nx-plugin-flowda/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/nx-plugin-flowda",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/nx-plugin-flowda",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/nx-plugin-flowda/**/*.ts",
          "libs/nx-plugin-flowda/generators.json",
          "libs/nx-plugin-flowda/executors.json",
          "libs/nx-plugin-flowda/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nx-plugin-flowda/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
